FROM ubuntu:16.04

RUN apt-get update
RUN apt-get install -y wget
RUN wget http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css
RUN wget https://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js
RUN wget https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js
RUN wget http://cdn.robotwebtools.org/ColladaAnimationCompress/current/ColladaLoader2.js
RUN wget http://cdn.robotwebtools.org/EventEmitter2/current/eventemitter2.min.js
RUN wget http://cdn.robotwebtools.org/roslibjs/current/roslib.js
RUN wget http://cdn.robotwebtools.org/ros3djs/current/ros3d.min.js
RUN wget http://cdn.robotwebtools.org/keyboardteleopjs/current/keyboardteleop.js
